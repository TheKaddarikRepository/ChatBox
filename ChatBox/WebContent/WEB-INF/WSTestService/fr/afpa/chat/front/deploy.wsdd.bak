<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from WSTestService WSDL service -->

  <service name="WSTest" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://front.chat.afpa.fr"/>
      <parameter name="wsdlServiceElement" value="WSTestService"/>
      <parameter name="schemaQualified" value="http://front.chat.afpa.fr"/>
      <parameter name="wsdlServicePort" value="WSTest"/>
      <parameter name="className" value="fr.afpa.chat.front.WSTestSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="WSTest"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="sayHello" qname="operNS:sayHello" xmlns:operNS="http://front.chat.afpa.fr" returnQName="retNS:sayHelloReturn" xmlns:retNS="http://front.chat.afpa.fr" returnType="rtns:string" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
      </operation>
      <parameter name="allowedMethods" value="sayHello"/>

  </service>
</deployment>
